{
  "build": {
    "content": [
      {
        "files": [
          "docs/**/*.{md,yml}",
          "design-patterns/**/*.{md,yml}" 
        ],
        "exclude": 
        [
          "docs/**/include/**",
          "docs/_site/**",
          "solution-architecture/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "docs/**/images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "docs/toc.yml"
        ]
      }
    ],
    "output": "docs/_site",
    "globalMetadata": {
      "_appLogoPath": ".",
      "_appTitle": "Data Solution Framework",
      "_enableSearch": "true",
      "_disableTocFilter": "true",
      "_disableBreadcrumb": "true",
      "_appFooter": "<span>Data Solution Framework</span>",
      "_disableContribution": "true"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false,
    "markdownEngineName": "markdig"
  }
}